.am-switch .checkbox{border-radius:50vh;border-radius:var(--am-switch-corner-radius,50vh)}.am-switch .a-switch-checkbox.a-switch-checkbox:checked~.checkbox{background:#1677ff;background:var(--am-switch-background,#1677ff)}.am-switch .checkbox:after{left:2px;top:2px;width:27px;height:27px;border-radius:50vh;border-radius:var(--am-switch-float-corner-radius,50vh)}.am-switch .checkbox:before{left:2px;top:2px;width:47px;height:27px;border-radius:50vh;border-radius:var(--am-switch-corner-radius,50vh)}